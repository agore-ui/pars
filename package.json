{
  "name": "hh-job-intelligence-tool",
  "version": "1.0.0",
  "description": "HH.ru job parser with AI lead scoring and outreach automation",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "NODE_ENV=development node src/index.js",
    "parser:test": "node src/parser/scraper.js",
    "db:init": "node src/scripts/setup-db.js",
    "db:migrate": "sqlite3 data/hh_intelligence.db < migrations/initial.sql",
    "pm2:start": "pm2 start ecosystem.config.js --name hh-intelligence-parser",
    "pm2:stop": "pm2 stop hh-intelligence-parser",
    "pm2:restart": "pm2 restart hh-intelligence-parser",
    "pm2:logs": "pm2 logs hh-intelligence-parser",
    "pm2:delete": "pm2 delete hh-intelligence-parser"
  },
  "keywords": [
    "hh.ru",
    "parser",
    "job-intelligence",
    "lead-scoring",
    "outreach"
  ],
  "author": "TQB Agency",
  "license": "MIT",
  "dependencies": {
    "puppeteer": "^21.0.0",
    "sqlite3": "^5.1.6",
    "dotenv": "^16.3.1",
    "axios": "^1.5.0",
    "nodemailer": "^6.9.6",
    "pm2": "^5.3.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.52.0",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
